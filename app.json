{
  "expo": {
    "name": "Game Knight",
    "slug": "game-knight",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/Game_Knight_App_Icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nyphon.myapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1",
      "appleTeamId": "H839Y86F94"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/Game_Knight_App_Icon.png",
        "backgroundImage": "./assets/images/Game_Knight_App_Icon.png",
        "monochromeImage": "./assets/images/Game_Knight_App_Icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.nyphon.myapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Game_Knight_App_Icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Game_Knight_App_Icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "63f9be2c-7c74-409a-924f-f5a2225302f1"
      }
    }
  }
}
